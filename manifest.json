{
  "manifest_version": 3,
  "name": "1bis Extension",
  "version": "1.0",
  "description": "A Chrome extension that monitors encode key button clicks and displays information",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": [
        "http://localhost:5173/*",
        "https://onebis-app-pro-max.web.app/*"
      ],
      "js": ["dist/content.js"]
    }
  ]
}
